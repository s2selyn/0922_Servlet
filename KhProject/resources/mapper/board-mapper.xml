<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boardMapper">

	<select id="selectListCount"
			resultType="_int">
		SELECT
		       COUNT(*)
		  FROM
		       KH_BOARD
		 WHERE
		       STATUS = 'Y'
		   AND
		       BOARD_TYPE = 1
	</select>

</mapper>