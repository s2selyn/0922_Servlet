<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body> 

    <jsp:include page="../include/header.jsp" />

    <div class="outer">

        <h2 align="center">사진게시글 작성하기</h2>
        <br><br>

        <form action="insert.image" 
            enctype="multipart/form-data"
            method="post" id="insert-form" style="width:800px;margin:auto;">
        
            <div class="form-group">
                <label for="usr">제목</label>
                <input type="text" class="form-control" id="usr" name="title">
            </div>

            <div class="form-group">
                <label for="comment">내용</label>
                <textarea class="form-control" name="content" rows="15" id="comment" style="resize:none;"></textarea>
            </div>

            <div class="form-group" align="center" >
                <label>대표이미지</label><br>
                <img width="300" id="title-img" height="180" src="data:image/png;base64," alt="대표이미지">
            </div>
            <div class="form-group" align="center" style="display:inline-block">
                <label>상세이미지-1</label><br>
                <img width="250" id="sub-img1" height="180" src="data:image/png;base64," alt="상세이미지1">
            </div>
            <div class="form-group" align="center" style="display:inline-block; margin-left:19px;">
                <label>상세이미지-2</label><br>
                <img width="250" id="sub-img2" height="180" src="data:image/png;base64," alt="상세이미지2">
            </div>
            <div class="form-group" align="center" style="display:inline-block; margin-left:19px;">
                <label>상세이미지-3</label><br>
                <img width="250" id="sub-img3" height="180" src="data:image/png;base64," alt="상세이미지3">
            </div>

            <div id="file-area">
                <input type="file" name="file1" id="file1" required onchange="loadImg(this, 1);">
                <input type="file" name="file2" id="file2" onchange="loadImg(this, 2);">
                <input type="file" name="file3" id="file3" onchange="loadImg(this, 3);">
                <input type="file" name="file4" id="file4" onchange="loadImg(this, 4);">
            </div>
            
            <script>
            	// jQuery로 숨기자, JS로 하면 document. 어쩌고 해서 길다
            	$(function() {
            		
            		$("#file-area").hide(); // 버튼을 숨기고
            		
            		// 이미지를 클릭하면
            		$("#title-img").click(() => {
            			$("#file1").click(); // 버튼이 클릭되도록 함
            		});
            		
            		$("#sub-img1").click(() => {
            			$("#file2").click();
            		});
            		
            		$("#sub-img2").click(() => {
            			$("#file3").click();
            		});
            		
            		$("#sub-img3").click(() => {
            			$("#file4").click();
            		});
            		
            	})
            </script>
            
            <script>
            	// jQuery 써보기~
            	function loadImg(e, num) {
            		
            		// console.log('첫번째인자 : ' + e + ", 두번째 : " + num);
            		// 첫번째는 input요소, 두번째는적어둔 숫자가 넘어온다
            		// 실질적으로 우리에게 중요한것은 e, 순수하게 얘만 찍어보자
            		// console.log(e);
            		// e : 현재 change이벤트가 발생한 <input type="file"> 요소객체
            		// num : 몇 번째 input요소인지 확인 후 해당영역에 미리보기할라고 받음
            		
            		// input요소에 files라는 속성이 있음
            		console.log(e.files);
            		// 0번 속성에 파일객체가 들어가있음, 첨부한 파일의 정보가 들어가있다
            		// 이름, 크기, 확장자
            		// length 속성도 있다, 첨부하면 1이 된다
            		// 첨부가 된것을 취소하면? 0번 속성이 없어지고 length 속성이 0이 된다
            		// 그럼 length 속성을 보면 첨부된건지, 첨부되어있던걸 취소한건지 알수있음
            		// files:첨부한 파일의 정보를 가지고있음
            		// files.length : 파일을 첨부 1, 선택 취소 0
            		// => 파일의 존재 유무를 파악할 수 있음
            		// 파일이 첨부되었을 경우 e.files[0]에 파일의 정보를 확인할 수 있음
            		
            		// 첨부되면 미리보기 보여주고싶음 -> 조건
            		if(e.files.length === 1) { // 파일첨부라는 뜻
            			
            			// 선택된 파일을 읽어서 이미지를 띄울 수 있는 URL을 만들 것
            			// 필요한 객체 == 자바스크립트의 FileReader 객체가 있음, 파일의 정보를 읽어온다
            			// 만드는건 생성자 호출하면 끝
            			const reader = new FileReader();
            		
            			// reader객체 메소드 중에 파일읽기 메소드 호출 -> URL을 만들어준다
            			// 매개변수로 파일의 정보, 아까 files의 0번 인덱스에 있었음
            			// 이러면 파일을 읽어서 url을 만들어준다, 파일을 띄울 수 있는 url
            			reader.readAsDataURL(e.files[0]);
            			
            			// 파일을 읽는 순간 파일의 고유한 URL이 생김
            			// 이걸 이미지 태그의 src 속성값에 바꿔넣을것임
            			// URL을 미리보기 영역의 img태그의 src속성값으로 부여할 것
            			
            			// 파일 읽기가 완료되면 실행할 이벤트 핸들러를 하나 정의
            			// 파일을 다 읽으면 onload 이벤트 발생임
            			reader.onload = function(e) {
            				
            				console.log(e);
            				// console > ProgressEvent > target > result > 겁나 긴 내가 첨부한 이미지파일에 대한 URL
            				// target 속성의 result 속성값을 빼내서 쓸것이다
            				// 직접 복사하기엔 너무 길어
            				const url = e.target.result;
            				// 1번 이미지에 작업했음, 그럼 이것에 미리보기를 보여주고싶음
            				// 각 인풋요소에 id가 있다
            				// 첫번째것의 id를 달겠다면?
            				// const titleImg = document.querySelector("#title-img");
            				// titleImg.src = url;
            				
            				// 각 영역에 맞춰 미리보기 해주기
            				// num값 확인해서 각각에 맞는 img태그에 넣어주기
            				switch(num) {
            				case 1 : $("#title-img").attr('src', url); break;
            				case 2 : $("#sub-img1").attr('src', url); break;
            				case 3 : $("#sub-img2").attr('src', url); break;
            				case 4 : $("#sub-img3").attr('src', url); break;
            				}
            				
            			}
            			
            		} else { // 파일 첨부 취소하면 파일 미리보기 없애기(바꿔치기)
            			
            			const tuna = "data:image/jpeg;base64,";
            			
            			switch(num) {
        				case 1 : $("#title-img").attr('src', tuna); break;
        				case 2 : $("#sub-img1").attr('src', tuna); break;
        				case 3 : $("#sub-img2").attr('src', tuna); break;
        				case 4 : $("#sub-img3").attr('src', tuna); break;
        				}
        				
            		}
            		
            	}
            
            </script>

            <div align="center" style="margin-top:20px">
                <button type="submit" class="btn btn-sm btn-info">등록하기</button>
                <button type="button" class="btn btn-sm btn-secondary"
                onclick="history.back();">뒤로가기</button>
            </div>

        </form>
        
    </div>
    
	<jsp:include page="../include/footer.jsp" />

	

</body>
</html>